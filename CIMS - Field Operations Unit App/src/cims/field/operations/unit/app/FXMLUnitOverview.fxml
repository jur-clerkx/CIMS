<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="400.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="cims.field.operations.unit.app.FXMLUnitOverviewController">
    <stylesheets>
        <URL value="@fxmlunitoverview.css" />
    </stylesheets>
   <children>
      <HBox prefHeight="150.0" prefWidth="400.0">
         <children>
            <VBox alignment="CENTER_RIGHT" prefHeight="246.0" prefWidth="150.0">
               <children>
                  <Label text="Unit ID:">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label text="Name:">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label text="Description:">
                     <VBox.margin>
                        <Insets bottom="50.0" />
                     </VBox.margin>
                  </Label>
                  <Label text="Shift:" />
               </children>
            </VBox>
            <VBox prefHeight="246.0" prefWidth="250.0">
               <children>
                  <TextField fx:id="textFieldUnitID" editable="false">
                     <VBox.margin>
                        <Insets top="2.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="textFieldUnitName" editable="false">
                     <VBox.margin>
                        <Insets top="2.0" />
                     </VBox.margin>
                  </TextField>
                  <TextArea fx:id="textAreaUnitDescription" editable="false" prefHeight="67.0" prefWidth="250.0">
                     <VBox.margin>
                        <Insets top="2.0" />
                     </VBox.margin>
                  </TextArea>
                  <TextField fx:id="textFieldUnitShift" editable="false">
                     <VBox.margin>
                        <Insets top="2.0" />
                     </VBox.margin>
                  </TextField>
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutY="160.0" prefHeight="235.0" prefWidth="400.0">
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="130.0">
               <children>
                  <Label text="Users" />
                  <ListView fx:id="listViewUnitUsers" prefHeight="224.0" prefWidth="133.0" />
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="130.0">
               <children>
                  <Label text="Materials" />
                  <ListView fx:id="listViewUnitMaterials" prefHeight="224.0" prefWidth="133.0" />
               </children>
               <HBox.margin>
                  <Insets left="3.0" right="3.0" />
               </HBox.margin>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="130.0">
               <children>
                  <Label text="Vehicles" />
                  <ListView fx:id="listViewUnitVehicles" prefHeight="224.0" prefWidth="133.0" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
